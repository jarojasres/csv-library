{
  "compilerOptions": {
      "outDir": "./dist/",
      "rootDir": "./src", 
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es6",
      "allowJs": true,
  },
  "include": [
      "./src/**/*"
  ]
}